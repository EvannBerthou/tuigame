FUNC ISPRIME(n);
    IF n < 2;
        RETURN 0;
    END
    FOR i IN 2..n;
        IF MOD(n i) == 0 AND i != n;
            RETURN 0;
        END
    END
    RETURN 1;
END

FOR k IN 0..100;
    IF ISPRIME(k);
        PRINTN(k);
    END
END
