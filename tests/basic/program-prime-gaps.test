FUNC ISPRIME(n);
    IF n < 2;
        RETURN 0;
    END
    FOR i IN 2..n;
        IF MOD(n i) == 0 AND i != n;
            RETURN 0;
        END
    END
    RETURN 1;
END

prev = 0;

FOR k IN 2..30;
    IF ISPRIME(k);
        IF prev != 0;
            PRINT(prev);
            PRINT(" ");
            PRINTN(k);
        END
        prev = k;
    END
END

