network: 0
files: 
- /x:PRINT "START";\nframe = 0;\nWHILE frame < 10;\n    FOR px IN 0..80;\n        FOR py IN 0..60;\n            PUTPIXEL px py px+py+frame/3;\n        END\n    END\n    RENDER;\n    frame = frame + 1;\nEND\n\nPRINT "END";\nPRINT "END";\nPRINT "END";\n
